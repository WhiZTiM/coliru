a = 5, b = 3
a / b = 1
(a / b) * b) = 3
a % b = 2
((a / b) * b) + (a % b) = 5

a = -5, b = 3
a / b = -1
(a / b) * b) = -3
a % b = -2
((a / b) * b) + (a % b) = -5

a = 5, b = -3
a / b = -1
(a / b) * b) = 3
a % b = 2
((a / b) * b) + (a % b) = 5

a = -5, b = -3
a / b = 1
(a / b) * b) = -3
a % b = -2
((a / b) * b) + (a % b) = -5

